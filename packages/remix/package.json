{
  "name": "@bundleup/remix",
  "version": "1.0.0",
  "description": "BundleUp plugin for Remix applications",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts",
    "dev": "tsup src/index.ts --format cjs,esm --dts --watch",
    "lint": "eslint src/",
    "test": "jest",
    "clean": "rm -rf dist"
  },
  "dependencies": {
    "@bundleup/common": "file:../common"
  },
  "peerDependencies": {
    "@remix-run/node": ">=1.0.0",
    "@remix-run/react": ">=1.0.0"
  },
  "devDependencies": {
    "tsup": "^7.0.0",
    "typescript": "^5.0.0",
    "eslint": "^8.0.0",
    "jest": "^29.0.0",
    "@remix-run/node": "^1.0.0",
    "@remix-run/react": "^1.0.0"
  },
  "files": [
    "dist"
  ],
  "keywords": ["bundleup", "remix", "plugin"],
  "author": "BundleUp",
  "license": "ISC"
}