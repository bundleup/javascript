{
  "name": "@bundleup/nextjs",
  "version": "0.0.7",
  "description": "BundleUp plugin for Next.js applications",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./react": {
      "types": "./dist/react.d.ts",
      "import": "./dist/react.mjs",
      "require": "./dist/react.js"
    }
  },
  "scripts": {
    "build": "tsup src/index.ts src/react.ts --format cjs,esm --dts",
    "dev": "tsup src/index.ts src/react.ts --format cjs,esm --dts --watch",
    "lint": "eslint src/",
    "test": "jest",
    "clean": "rm -rf dist"
  },
  "dependencies": {
    "@bundleup/core": "0.0.5",
    "@bundleup/react": "0.0.7"
  },
  "peerDependencies": {
    "next": ">=13.0.0"
  },
  "devDependencies": {
    "tsup": "^7.0.0",
    "typescript": "^5.0.0",
    "eslint": "^8.0.0",
    "jest": "^29.0.0",
    "next": "^13.0.0"
  },
  "files": [
    "dist"
  ],
  "author": "BundleUp",
  "license": "ISC"
}