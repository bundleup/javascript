{
  "name": "@bundleup/javascript-monorepo",
  "version": "0.0.0",
  "description": "Official JavaScript repository for BundleUp - Turbo Monorepo",
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev",
    "lint": "turbo run lint",
    "test": "turbo run test",
    "clean": "turbo run clean",
    "changelog": "npx @changesets/cli",
